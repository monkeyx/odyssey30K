  background-position: center center;
  text-align: left;
}
/* end Header */

/* begin Logo */
.logo
{
  position: relative;
  left: 320px;
  top: 40px;
}

.logo td
{
  text-align: center;
  vertical-align: middle;
  white-space: nowrap;
}

.logo-name
{
  font-size: 28px;
  font-family: Georgia;
  font-style: italic;
  font-weight: bold;
}

.logo-name a
{
  text-decoration: none;
  color: #F5F9FA !important;
}

.logo-text
{
  font-size: 20px;
